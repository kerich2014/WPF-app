<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stroymaterials"
        xmlns:AppData="clr-namespace:Stroymaterials.AppData" x:Class="Stroymaterials.MainWindow"
        mc:Ignorable="d"
        Icon="pack://application:,,,/Resources/logo.png"
        Title="АвтоМастер" Height="600" Width="800">

    <Grid x:Name="grid_main" Background="#FFA0A0A0">
        <Grid.RowDefinitions>
            <RowDefinition Height="135*"/>
            <RowDefinition Height="465*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="main_frame" Content="" HorizontalAlignment="Center" Height="465" VerticalAlignment="Top" Width="800" NavigationUIVisibility="Hidden" Margin="0,119,0,0" Grid.RowSpan="2" Background="#FFA0A0A0"/>
        <Image HorizontalAlignment="Left" Source="/Resources/logo1.png" Height="121" VerticalAlignment="Top" Width="473"/>
        <Frame x:Name="frame_name" Content="" Margin="450,25,20,20"  Width="250" Height="80" Foreground="{x:Null}"/>
    </Grid>

</Window>
